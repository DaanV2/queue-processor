{
  "name": "@daanv2/queue-processor",
  "version": "1.0.0",
  "description": "A promise like processor that tries to keep the event-loop as clean as possible",
  "main": "./lib/src/main.js",
  "types": "./lib/src/main.d.ts",
  "scripts": {
    "compile": "tsc -b",
    "build": "npm run clean && npm run compile",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "test": "mocha --debug-brk",
    "clean": "rimraf lib",
    "pretest": "npm run compile",
    "prepublishOnly": "npm test",
    "preversion": "",
    "postversion": "git push && git push --tags",
    "version": "git add -A src",
    "watch": "tsc -w -p ./src"
  },
  "keywords": [
    "promise",
    "queue",
    "processor"
  ],
  "author": "DaanV2",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DaanV2/queue-processor/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaanV2/queue-processor"
  },
  "homepage": "https://github.com/DaanV2/queue-processor#readme",
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.0",
    "chai": "^4.3.4",
    "mocha": "^9.1.3",
    "rimraf": "^3.0.2",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4"
  }
}
